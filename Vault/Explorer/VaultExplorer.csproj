  <!-- 
   Build ClearClipboard.csproj and copy the generated .exe with new extension .ex_, all this to avoid ClickOnce
   warning MSB3178: Assembly '..\ClearClipboard\bin\Debug\ClearClipboard.exe' is incorrectly specified as a file.
  -->
  <ItemGroup>
    <Content Include="..\ClearClipboard\bin\$(Configuration)\ClearClipboard.ex_">
      <Link>ClearClipboard.exe</Link>
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
      <Visible>False</Visible>
    </Content>
    <ClearClipboardExe Include="..\ClearClipboard\bin\$(Configuration)\ClearClipboard.exe">
      <Visible>False</Visible>
    </ClearClipboardExe>
  </ItemGroup>
  <Target Name="BuildAndCopyClearClipboard" BeforeTargets="BeforeBuild">
    <MSBuild Projects="..\ClearClipboard\ClearClipboard.csproj" Targets="Build" />
    <Copy SourceFiles="@(ClearClipboardExe)" DestinationFiles="@(ClearClipboardExe->'..\ClearClipboard\bin\$(Configuration)\%(Filename).ex_')" ContinueOnError="false" />
  </Target>
